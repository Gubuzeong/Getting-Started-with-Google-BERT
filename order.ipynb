        "from collections import Counter\n",
        "\n",
        "random.seed(220112)\n",
        "\n",
        "names = ['수연', '상일', '희', '은식']\n",
        "orders = ['은식']\n",
        "\n",
        "random.shuffle(names[:2])\n",
        "orders += names\n",
        "\n",
        "while len(orders) < 21:\n",
        "    random.shuffle(names)\n",
        "    orders += names\n",
        "\n",
        "orders = orders[:21]\n",
        "print(orders)\n",
        "\n",
        "Counter(orders)"
